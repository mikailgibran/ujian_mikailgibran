
@{
    ViewBag.Title = "SETIA MULIA";
    ViewBag.dashboard = "Laporan";
    ViewBag.dashboardSmall = "Laporan";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
    ViewBag.pathParent = Url.Content("~").Substring(0, Url.Content("~").Length - 1);
}

<input type="hidden" id="urlPath" name="urlPath" value="@ViewBag.pathParent" />

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">

                <div class="row">
                    <div class="col-md-1">
                        <label class="control-label" style="text-align:left" for="periode">Periode</label>
                    </div>
                    <div class="col-md-2">
                        <input type="text" class="form-control" style="width:90%" id="txt_periode" name="periode">
                    </div>
                    <div class="col-md-2">
                        <button class="btn btn-xs btn-primary" style="padding: 10px 24px;font-size:14px;" id="btn_view"><i class="" aria-hidden="true"></i> View</button>
                    </div>
                </div>
                <br />

                <div class="row">
                    <div class="col-md-12">
                        <div id="grid"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="@ViewBag.pathParent/Js/Laporan.js"></script> 

