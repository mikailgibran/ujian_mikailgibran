<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login</title>
    @{
        ViewBag.pathParent = Url.Content("~").Substring(0, Url.Content("~").Length - 1);
        Layout = null;
    }
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="@ViewBag.pathParent/ContectLayout/Login/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/vendor/bootstrap/css/bootstrap.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/fonts/iconic/css/material-design-iconic-font.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/vendor/animate/animate.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/vendor/css-hamburgers/hamburgers.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/vendor/animsition/css/animsition.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/vendor/select2/select2.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/vendor/daterangepicker/daterangepicker.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/css/util.css" />
    <link rel="stylesheet" type="text/css" href="@ViewBag.pathParent/ContectLayout/Login/css/main.css" />
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('@ViewBag.pathParent/ContectLayout/Login/images/bg-01.jpg');">
            <div class="wrap-login100">
                <form class="login100-form validate-form" action="@ViewBag.pathParent/login/getUser" method="POST" name="fom_login" id="fom_login">
                    <span class="login100-form-logo">
                        <img src="@ViewBag.pathParent/ContectLayout/Login/images/logo-light-text.png" />
                    </span>

                    @if (TempData["notice"] != null)
                    {
                        <div class="row" style="padding:0 50px;">

                            <div class="alert alert-danger">
                                <a class="close" data-dismiss="alert">×</a>
                                <strong>Perhatian! </strong> @Html.Raw(TempData["notice"])
                            </div>
                        </div>
                    }

                    <div class="wrap-input100 validate-input" data-validate="Enter Username">
                        <input class="input100" type="text" name="username" id="username" placeholder="Username" />
                        <span class="focus-input100" data-placeholder="&#xf207;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <input class="input100" type="password" name="password" id="password" placeholder="Password" />
                        <span class="focus-input100" data-placeholder="&#xf191;"></span>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            Login
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/bootstrap/js/popper.js"></script>
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/daterangepicker/moment.min.js"></script>
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="@ViewBag.pathParent/ContectLayout/Login/js/main.js"></script>

</body>
</html>